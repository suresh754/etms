    <script src="ext/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="ext/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="ext/js/main.js"></script>

  </body>
</html>
